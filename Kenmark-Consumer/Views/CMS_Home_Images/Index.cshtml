    <img src="@Url.Content("~/Content/images/cms/home-images.png")" class="img-responsive img-circle"  style="max-width:450px; margin:auto;"/>
  
</div>


<div class="row" style="margin-bottom:40px;">    
       
        <div class="col-xs-12 col-sm-12 text-center divider" style="margin-bottom:20px;padding:30px;border: 1px solid #ccc;">
            <a href="~/CMS_Home_Images/Edit">
                <span style="font-size:26px;" class="glyphicon glyphicon-minus"></span>
                <div class="pageTitle" id="edit"><span style="font-size:20px">Edit an Image</span></div>
            </a>
        </div>

       
   
    </div>
