    <script type="text/javascript">
        var is_mobile = false;
        $(function () {

            $(window).on('resize', function () {
                if ($('#mobile-test').is(':visible')) {
                    is_mobile = true;
                }

                if (is_mobile) {
                    $('.navbar-inner div').css('padding', 0);
                    $('#logo-sprite').css('margin-right', 0);
                }
                var h = $('.navbar').height() - 5;
                $('#main-body').css('margin-top', (h));
            });


            if ($('#mobile-test').is(':visible')) {
                is_mobile = true;
            }

            if (is_mobile) {
                $('.navbar-inner div').css('padding', 0);
                $('#logo-sprite').css('margin-right', 0);
            }

            
            var h = $('.navbar').height() - 5;
            $('#main-body').css('margin-top', (h));
            
            $('#Brands-Link').click(function () {
               if ($('#brand-links-div').is(':visible')) {
                    $('.navbar-inner .container').addClass('shadow');
                }
                else {
                    $('.navbar-inner .container').removeClass('shadow');
                }
                $("#brand-links-div").fadeToggle(100);

                if ($('#profile-links-div').is(':visible')) {
                    $('#profile-links-div').hide();
                }
               
            });

           

            $('#Profile-Link').click(function () {
                
                if ($('#profile-links-div').is(':visible')) {
                    $('.navbar-inner .container').addClass('shadow');
                }
                else {
                    $('.navbar-inner .container').removeClass('shadow');
                }
                $("#profile-links-div").fadeToggle(100);

                if($('#brand-links-div').is(':visible')){
                    $('#brand-links-div').hide();
                }
            });


           
            $('.collection-link-text').hover(function (e) {
                $(this).prev().removeClass('desaturate').addClass('saturate');
            })

            $('.collection-link-text').mouseleave(function (e) {
                $(this).prev().removeClass('saturate').addClass('desaturate');
            })

            $('.browseText').mouseenter(function () {
                $(this).css("color", "black");
            })

            .mouseleave(function () {
                $(this).css("color", "#64635e");
            });


            
        });
        
       

    </script>
